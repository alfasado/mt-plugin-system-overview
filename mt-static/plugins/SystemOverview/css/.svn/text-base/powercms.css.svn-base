/**
 * ==============================================
 * FIXED POSITION HEADER 
 * ==============================================
 */
	 
body 
{
	padding-top: 64px;
}
 
#header 
{
	position: fixed;
	top: 0;
	left: 0;

	height: 64px;
	width: 100%;
	z-index: 1000;
	/*
	overflow: hidden;
	*/
}
 
/**
 * ==============================================
 * LOGO SPACE (#brand) 
 * ==============================================
 */
	 
#brand 
{
	height: 35px;
	width: 100%;
}
 
#brand a 
{
	top: 0;
}
	
#brand a:hover 
{
	background-color: #2b2b2b;
}
  
#brand span.tips 
{
	display: none !important;
}
 
#brand h1 
{
	padding-top: 5px;
	padding-left: 203px;
	height: 30px;
	line-height: 30px;
	color: #FFF;
	font-size: 14px;
}

* html #brand h1#page-title span {
	background: none !important;
}
  
/**
 * ==============================================
 * UTILITY (#utility-nav) 
 * ==============================================
 */
	 
#utility-nav 
{
	position: fixed;
	top: 7px;
	z-index: 1000 !important;
}
	 
#utility-nav #help 
{
	margin-right: 1px;
}
 
#utility-nav #user 
{
	margin-right: 0;
}
	 
#utility-nav #user a 
{
	padding-right: 0;
	border-right: none;
}
    
/**
 * ==============================================
 * MENU BAR (#menu-bar) 
 * ==============================================
 */
	 
#menu-bar, 
#selector-nav .current
{
	height: 29px;
}
 
#selector-nav .selector 
{
	top: 29px;
	left: 33px;
	z-index: 1000;
}

 
#selector-nav-list .current em 
{
	line-height: 29px;
	height: 29px;
}


#menu-bar-nav 
{
	top: 3px;
	width: 500px !important;
}

 
#display-options-widget 
{
	display: none;
	position: fixed !important;
	top: 64px;
}

* html #display-options-widget {
	position: absolute !important;
}

 
.fav-actions 
{
	z-index: 501 !important;
}

* html .fav-actions-nav .fav-actions,
* html .fav-actions-nav .fav-actions li,
* html .fav-actions-nav .fav-actions li a {
	width: 91px;
}
  
#footer 
{
	position: absolute;
}

  
/**
 * ==============================================
 * PowerRevision Style 
 * ==============================================
 */
	 
.listing .object-status, 
.listing .revision-status,
.listing .recover
{
	padding-right: 0;
	padding-left: 0;
	text-align: center;
	width: 3.5em;
}
 
.listing .object_name 
{
	width: 20%;
}
  
/**
 * ==============================================
 * Icons 
 * ==============================================
 */
	
.icon-view 
{
	background-image: url(../../../images/status_icons/view.gif);
}
 
.duplicate-button 
{
	margin-left: 5px;
}
 
.listing img 
{
	vertical-align: top;
}
	 
.listing .object_name img 
{
	vertical-align: middle;
}
 
.listing th.revisions 
{
	width: 6em;
}
 
.listing td.revisions 
{
	padding-right: 0;
	padding-left: 0;
	width: 2em;
	text-align: center;
}
	 
.listing td.revisions span 
{
	padding-right: 10px;
}
 
.listing .revisions 
{
/*
	vertical-align: middle;
*/
}
  
.listing th.category, 
.listing th.author,
.listing th.datetime
{
	width: 10%;
}
 
.listing th.view 
{
	padding-right: 0;
	padding-left: 0;
	width: 3em;
}
   
/**
 * ==============================================
 * Header create menu 
 * ==============================================
 */
	 
#utility-nav .extended_menu 
{
	position: relative;
	z-index: 1050 !important;
}
	 
#utility-nav .extended_menu .extended_menu_a 
{
	height: 28px;
	line-height: 28px;
	padding: 0 17px 0 10px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	-khtml-border-top-left-radius: 5px;
	-khtml-border-top-right-radius: 5px;
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 5px;
	background-image: url(../images/arrow-down-gray.gif);
	background-position: right center;
	background-repeat: no-repeat;
}
	 
#utility-nav .extended_menu .extended_menu_a:hover, 
#utility-nav .hover .extended_menu_a
{
	color: #EAE69F;
	background-color: #575757;
	background-image: url(../images/arrow-down-gray_on.gif) !important;
	text-decoration: none;
}
  
#utility-nav .extended_menu ul 
{
	display: none;
	position: absolute;
	top: 28px;
	left: 0;
	width: 150px;
	z-index: 1000;
}
	 
#utility-nav .extended_menu ul li 
{
	display: block;
	position: relative;
	float: left;
	width: 150px;
	height: 25px;
}
 
#utility-nav .hover ul.extended_menu_ul 
{
	display: block !important;
}
 
#utility-nav .extended_menu ul li a, 
#utility-nav .extended_menu ul li span
{
	display: block;
	padding: 0 10px;
	width: 130px;
	height: 25px;
	line-height: 25px;
	color: #C8C2BE;
	background-color: #575757;
/*	cursor: pointer;*/
	overflow: hidden;
}

* html #utility-nav .extended_menu ul li,
* html #utility-nav .extended_menu ul li a, 
* html #utility-nav .extended_menu ul li span {
	zoom: 1;
}
 
#utility-nav .extended_menu ul li span 
{
	background-image: url(../images/arrow-right-gray.gif);
	background-position: right center;
	background-repeat: no-repeat;
}
	
#utility-nav .extended_menu ul .li_hover span 
{
	background-image: url(../images/arrow-right-gray_on.gif) !important;
}
  
#utility-nav .extended_menu ul li a:hover, 
#utility-nav .extended_menu ul .li_hover span.trigger
{
	color: #EAE69F;
	text-decoration: none;
	background-color: #454545;
}
  	
#utility-nav .extended_menu ul ul 
{
	top: 0px;
	left: 150px;
	float: left;
}
	 
#utility-nav .extended_menu ul .li_hover ul 
{
	display: block !important;
}
   
#utility-nav #bookmarks_menu .extended_menu_a, 
#utility-nav #bookmarks_menu .extended_menu_a:hover
{
	padding-right: 22px;
	background-image: url(../../BlogTree/images/shortcut.gif) !important;
	background-position: 95% center ;
}
	 
#utility-nav #bookmarks_menu ul, 
#utility-nav #bookmarks_menu li
{
	width: 220px;
}
	 
#utility-nav #bookmarks_menu ul li a, 
#utility-nav #bookmarks_menu ul li span
{
	width: 200px;
}
    
