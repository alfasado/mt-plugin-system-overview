	margin-bottom: 5px;
	text-align: right;
}
 
.action-link-list 
{
	text-align: left !important;
}
 
.object-nav 
{
	position: static;
	margin-right: 260px;
	margin-bottom: 5px;
	text-align: right;
}
 
#msg-block 
