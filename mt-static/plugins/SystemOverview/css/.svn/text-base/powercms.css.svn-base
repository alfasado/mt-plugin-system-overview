/**
 * Object Link 
 * ==============================================
 */
	
#show_display_options_link, 
.object-nav
{
	float: right;
	margin-right: 10px;
}
 
#show_display_options_link a, 
.object-nav a
{
	position: static;
	color: #eae69f;
}
	 
#show_display_options_link a:hover, 
.object-nav a:hover,
#show_display_options_link a.display-options-link-open
{
	color: #eae69f;
	background-color: #949597 !important;
}
 
#show_display_options_link span, 
.object-nav span
{
	color: #d9d9d9;
}

 
.blog #show_display_options_link a:hover, 
.blog .object-nav a:hover,
.blog #show_display_options_link a.display-options-link-open
{
	background-color: #7da5ad !important;
}
  
#show_display_options_link a 
{
	background-image: url(../images/bg_options_link.gif);
}
	 
#show_display_options_link a:hover 
{
	background-image: url(../images/bg_options_link_hover.gif);
}
 
#show_display_options_link a.display-options-link-open 
{
	background-image: url(../images/bg_options_link_open.gif);
	background-position: 8px center;
}
 
.blog #show_display_options_link a 
{
	background-image: url(../images/bg_options_link_blog.gif);
}
	 
.blog #show_display_options_link a:hover 
{
	background-image: url(../images/bg_options_link_blog_hover.gif);
}
 
.blog #show_display_options_link a.display-options-link-open 
{
	background-image: url(../images/bg_options_link_blog_open.gif);
	background-position: 8px center;
}
   
