@charset "UTF-8"; 
 
html,body 
{
	background-color: #f3f3f3;
}
 
.target 
{
	display: none;
}
 
#tab_item 
{
/*
	position: relative;
*/
}
	 
#tab_item .asset_list_box 
{
	display: none;
}
	 
#tab_item .asset_list_box 
{
	overflow-y: auto;
}
 
.asset_context 
{
	position: absolute;
	z-index: 2000;
	font-weight: normal;
	border: 1px solid #ddd;
	background-color :#f3f3f3;
}
	 
.asset_context a 
{
	display: block;
	padding: 5px;
	text-decoration: none;
}
 
.asset_context a:hover 
{
	color: #FFF;
	background-color: #7B7C7D;
}

  
#tab_item .asset_list_box .indicate 
{
	text-align: center;
}
 
#tab_item .asset_list_box .loading 
{
	height: 10px;
}
 	 
#tab_item #asset_all_list 
{
	display: block;
}
 
#tab_item .asset_list_box img 
{
	vertical-align: middle;
}

#tab_item .asset_list_box .thumbnail {
	height: 84px;
}

#tab_item .asset_list_box .thumbnail .thumbnail_image {
	height: 60px;
}
 
#tab_item .asset_list_box .thumbnail img 
{
	max-width: 60px;
	max-height: 60px;
	-ms-interpolation-mode: bicubic;
}
	 
* html body #tab_item .asset_list_box .thumbnail img 
{
	width: expression(this.width >= this.height ? "60px" : "auto");
	height: expression(this.width <= this.height ? "60px" : "auto");
}
  
#tab_item #drop 
{
	position: absolute;
	top: auto;
	bottom: 0;
	width: 100%;
	height: 45px;
	text-indent: -10000px;
	overflow: hidden;
	background: #b4b4b4 url(../images/ico_drop.gif) center center no-repeat;
}
	
#tab_item .hover_box 
{
	background-color: #ccc !important;
}
  
#tab_item .multi_upload 
{
	position: absolute;
	top: auto;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 45px;
	text-align: center;
	font-size: 10px;
	background-color: #F3F3F3;
	overflow: hidden;
}
	 
#tab_item .multi_upload .trigger 
{
	margin-top: 10px;
	height: 25px;
	line-height: 25px;
	background-color: #f6f6f6;
	cursor: pointer;
}
	 
#tab_item .multi_upload .open .trigger 
{
	margin-right: 10px;
	margin-bottom: 8px;
	margin-left: 10px;
	border: 1px solid #c1c1c1;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	-khtm-border-radius: 4px;
}
 
#tab_item .multi_upload .open .buttons 
{
	display: none;
}
 
#tab_item .multi_upload .trigger span 
{
	padding-left: 15px;
	background: url(../images/ico_multiupload.gif) left center no-repeat;
}
  
#tab_item .multi_upload .open .input 
{
	display: none;
}
 
#tab_item .multi_upload .close 
{
	margin: 10px;
	border: 1px solid #c1c1c1;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	-khtm-border-radius: 4px;
}
	 
#tab_item .multi_upload .close .trigger 
{
	margin-top: 0;
}
 
#tab_item .multi_upload .close .input 
{
	display: block;
}
  
#tab_item .multi_upload .sidebar_blocks 
{
	position: relative;
	padding-right: 16px;
	padding-left: 14px;
	margin: 0 10px;

}
	 
#tab_item .multi_upload .sidebar_blocks .disabled 
{
	background-color: #EEE;
	color: #999;
}
 
#tab_item .multi_upload .sidebar_blocks img 
{
	position: absolute;
	top: 4px;
	left: 0;
}
 
#tab_item .multi_upload .sidebar_blocks .file_name 
{
	font-size: 10px;
}
 
#tab_item .multi_upload .sidebar_blocks span 
{
	position: absolute;
	top: 1px;
	right: 0;
	display: block;
	width: 11px;
	height: 17px;
	background: url(../images/ico_multiupload.gif) 0 0 no-repeat;
	overflow: hidden;
	cursor: pointer;
}
	 
#tab_item .multi_upload .sidebar_blocks .select_icon 
{
	top: 0;
	right: 0;
	left: auto;
	display: none;
}
 
#tab_item .multi_upload .sidebar_blocks span input 
{
    position: absolute;
    right: 0px;
    height: 100%;
	cursor: pointer;
    opacity: 0;
    -moz-opacity: 0;
    filter:progid:DXImageTransform.Microsoft.Alpha(opacity=0);
}

#tab_item .multi_upload * {
}


   
#tab_item .MultiFile-list 
{
	text-align: left;
}
	 
#tab_item .MultiFile-list li 
{
	padding: 2px 10px;
	margin-bottom: 0;
}
	 
#tab_item #upload_form .even, 
#tab_item #upload_form .MultiFile-wrap,
#tab_item #upload_form p
{
	background-color: #FFF;
}
 
#tab_item #upload_form .odd 
{
	background-color: #f3f3f3;
}
   
#tab_item .multi_upload p 
{
	margin-bottom: 0;
	padding: 3px 0;
}

#tab_item .multi_upload p {
	background-color: #F6F6F6 !important;
}
	 
#tab_item .multi_upload p button 
{
	font-size: 10px;
	padding: 3px 5px;
	margin: 0px 2px
}
    
.asset_list_box 
{
	padding: 10px 0;
	margin-right: auto;
	margin-left: auto;
	overflow: auto;
	_zoom: 1;
}
	 
.asset_list_box table 
{
	float: left;
	margin: 0px;
	margin-bottom: 6px;
	zoom: 1;
	border-collapse:collapse;
	border-spacing: 0px;
}

.asset_list_box table table {
	margin: 0px;
}

.asset_list_box table.odd {
	clear: left;
	float: left;
}
	 
.asset_list_box table td 
{
	vertical-align: middle;
	text-align: center;
	padding: 0px;
}
 
.asset_list_box table .thumbnail img 
{
	vertical-align: middle;
}

.asset_list_box table .thumbnail_footer {
	text-align: center;
}

.asset_list_box table .thumbnail_footer div.file_name {
	margin:auto;
	text-align:center;
	font-size:12px;
	height: 15px;
	padding-top: 0px;
	overflow:hidden;
	white-space:nowrap;
}
 
.asset_list_box table .icons 
{
	width: 17px;
}
	 
.asset_list_box table .icons img 
{
	margin-bottom: 4px;
}
   
.asset_list_box table.image .thumbnail 
{
/*	height: 65px;*/
}
	 
.asset_list_box table.image .thumbnail img 
{
	box-shadow: 2px 2px 2px #555;
	-webkit-box-shadow: 2px 2px 2px #555;
}
  
.asset_list_box table.file, 
.asset_list_box table.video,
.asset_list_box table.audio
{
}
	 
.asset_list_box table.file .thumbnail, 
.asset_list_box table.video .thumbnail,
.asset_list_box table.audio .thumbnail
{
	vertical-align: middle;
}
	 
.asset_list_box table.file .thumbnail a, 
.asset_list_box table.video .thumbnail a,
.asset_list_box table.audio .thumbnail a
{
	display: block;
	margin: 0 auto;
	width: 65px;
	height: 65px;
	overflow: hidden;
/*	text-indent: -9999px;*/
	background-color: #EEE;
	background-position: center center;
	background-repeat: no-repeat;
}
	
.asset_list_box table.file .thumbnail a span, 
.asset_list_box table.video .thumbnail a span,
.asset_list_box table.audio .thumbnail a span
{
	filter: alpha(opacity=0);
	-moz-opacity:0;
	opacity:0;
	position: absolute;
}
  
.asset_list_box table.ic-file-default .thumbnail a,
.asset_list_box table.ic-video-default .thumbnail a, 
.asset_list_box table.ic-audio-default .thumbnail a,
{
