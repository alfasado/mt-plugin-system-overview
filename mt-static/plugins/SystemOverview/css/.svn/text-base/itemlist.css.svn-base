.asset_tab 
{
	padding-left: 25px;
	background-color: #b4b4b4;
	overflow: hidden;
	_zoom: 1;
}
	 
.asset_tab li 
{
	float: left;
	padding: 5px 0;
	margin: 0;
	width: 30px;
	text-align: center;
}
	 
.asset_tab li img 
{
	vertical-align: top;
}
  
.asset_tab li.current 
{
	background: url(../images/asset_current.gif) center bottom no-repeat;;
}
  
